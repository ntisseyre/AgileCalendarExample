@using AgileCalendarExample.Models.DomainModels

@model ReleaseCycleModel

<div class="agile-releaseCycle ui-widget-content">
    <div class="agile-releaseCycle-header ui-state-default">Planning</div>
    @Html.EditorFor(m => m.Planning)

    <div class="agile-releaseCycle-header ui-state-default">Sprints</div>
    @foreach (Sprint sprint in Model.Sprints)
    {
        @Html.EditorFor(m => sprint)
    }
    <div class="agile-releaseCycle-header ui-state-default">Holidays</div>
    @foreach (Holiday holiday in Model.Holidays)
    {
        @Html.EditorFor(m => holiday)
    }
    <div class="agile-releaseCycle-header ui-state-default">Vacations</div>
    @foreach (Holiday holiday in Model.Holidays)
    {
        @Html.EditorFor(m => holiday)
    }
</div>
@Html.Partial("ColorsGrid")